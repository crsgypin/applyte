.landing-body{
  padding: 0px;
  margin: 0px;
  min-width: 1024px;
  font-family: Arial, Helvetica, sans-serif;

  .header {
      width: 100%;
      min-width: 650px;
      height: 100px; /* kane 70px */
      padding-top: 30px;
      padding-left: 20px;
  }

  .wrapper {
      width: 100%;
  }

  .header .header-logo {
    float: left;
    width: 210px;
    height: 55px;
    background-image: url(<%=image_url("logo_landing.png")%>);
    /*background-image: url("../img/logo_landing.png");*/
    background-repeat: no-repeat;
    background-size: 100%;
  }

  .header .header-buttons {
    text-decoration: none;
    float: right;
    line-height: 40px;
    width: 450px;
  }

  .header-buttons .header-buttons-button {
    float: left;
    text-decoration: none;
    background-image: url(<%=image_url("bt01_bg.png")%>);
    background-repeat: no-repeat;
    background-size: 100%;

    color: #ffffff;
    text-align: center;
    vertical-align: middle;

    width: 180px;
  }

  .header-buttons .header-buttons-button:hover {
    float: left;
    background-image: url(<%=image_url("bt01_bg_hover.png")%>);
    background-repeat: no-repeat;
    background-size: 100%;

    color: #ffffff;
    text-align: center;
    vertical-align: middle;

    width: 180px;
  }
}