<div class="my_programs-index-box  contents col-md-offset-1 col-md-11">
	<ul class="nav nav-tabs" role="tablist">
    <li role="presentation" class="tab-wait"><a href="#tab-wait" aria-controls="home" role="tab" data-toggle="tab">Wish List</a></li>
    <li role="presentation" class="active tab-inprogess"><a href="#tab-inprogess" aria-controls="profile" role="tab" data-toggle="tab">In Progess</a></li>
    <li role="presentation" class="tab-compelete"><a href="#tab-compelete" aria-controls="messages" role="tab" data-toggle="tab">Compelete</a></li>
  </ul>
 	<div class="progress-list tab-content">
    <div role="tabpanel" class="tab-pane" id="tab-wait">
			<% @wish.each do |pf| %>
				<%= render :partial => "program_row", :locals => { :pf => pf } %>
			<% end %>
    </div><!--tab-wait-->
    <div role="tabpanel" class="tab-pane active" id="tab-inprogess">
			<% if current_user %>
				<% @progress.each do |pf| %>
					<%= render :partial => "program_row", :locals => { :pf => pf } %>
					<%= render :partial => "program_sub_row", :locals => { :pf => pf,:form_key_categories=>@form_key_categories } %>
				<% end %>
			<% end %>
    </div><!--tab-inprogess-->
    <div role="tabpanel" class="tab-pane" id="tab-compelete">
			<% if current_user %>
			<% @finish.each do |pf| %>
				<%= render :partial => "program_row", :locals => { :pf => pf } %>
			<% end %>
			<% end %>
    </div><!--tab-compelete-->

  </div>


</div>
<script>
	$(".info").on('click', function(event) {
		var d =$(this).children('.department').text();
		$("#main-department").text(d);
	});
	$(".detail").on('click', function(event) {
		$(".sub-percen").toggle();
	});
</script>