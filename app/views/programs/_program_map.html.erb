<%# :locals=>{:address=>address} %>
<div class="map">
	<h3>address: <%=address%></h3>
	<iframe
	  width="600"
	  height="450"
	  frameborder="0" style="border:0"
	  src="https://www.google.com/maps/embed/v1/place?key=AIzaSyDDaizNeCpDvvl_aB_vmak1SgBSKuIaEY4&q=<%=address_escape address%>&zoom=7" allowfullscreen>
</iframe>
</div>
