<%= nested_form_for setup_profile(@profile), :url => profile_path do |f| %>
	<div class="profile">
		<p>profile</p>
		<p class="form-group">
		<%= f.label :name,"name"%>
		<%= f.text_field :name %>
		</p>
		<p class="form-group">
		<%= f.label :birthday,"birthday"%>
		<%= f.text_field :birthday %>
		</p>
		<p class="form-group">
		<%= f.label :major,"major"%>
		<%= f.text_field :major %>
		</p>
		<p class="form-group">
		<%= f.label :research_area,"research_area"%>
		<%= f.text_field :research_area %>
		</p>
	</div>
	<div class="toefl_score">
		<p>toefl_score</p>
		<p class="form-group">
		<%= f.label :toefl_read,"toefl_read"%>
		<%= f.text_field :toefl_read %>
		</p>
		<p class="form-group">
		<%= f.label :toefl_listen,"toefl_listen"%>
		<%= f.text_field :toefl_listen %>
		</p>
		<p class="form-group">
		<%= f.label :toefl_speak,"toefl_speak"%>
		<%= f.text_field :toefl_speak %>
		</p>
		<p class="form-group">
		<%= f.label :toefl_write,"toefl_write"%>
		<%= f.text_field :toefl_write %>
		</p>
		<p class="form-group">
		<%= f.label :toefl_total,"toefl_total"%>
		<%= f.text_field :toefl_total %>
		</p>
	</div>
	<div class="gre_score">
		<p>gre_score</p>
		<p class="form-group">
		<%= f.label :gre_verbal,"gre_verbal"%>
		<%= f.text_field :gre_verbal %>
		</p>
		<p class="form-group">
		<%= f.label :gre_guantitatiue,"gre_guantitatiue"%>
		<%= f.text_field :gre_guantitatiue %>
		</p>
		<p class="form-group">
		<%= f.label :gre_awa,"gre_awa"%>
		<%= f.text_field :gre_awa %>
		</p>
		<p class="form-group">
		<%= f.label :gre_total,"gre_total"%>
		<%= f.text_field :gre_total %>
		</p>
	</div>
	<div class="publication">
		<div class="publication_item">
			<p>publication</p>
			<%= f.nested_fields_for :publication do |publication_form| %>
			<p class="form-group">
				<%= publication_form.label :title, "title" %>
				<%= publication_form.text_field :title %>
			</p>
			<p class="form-group">
				<%= publication_form.label :url, "url" %>
				<%= publication_form.text_field :url %>
			</p>
			<% end %>
		</div>
		<%=link_to '+','',{:id=>:publication_button}%>
	</div>
	<div class="honor">
		<div class="honor-item">
			<p>honor</p>
			<%= f.fields_for :honor do |honor_form| %>
			<p class="form-group">
				<%= honor_form.label :title, "title" %>
				<%= text_field_tag 'profile_attribute[honor][][title]' %>
			</p>
			<p class="form-group">
				<%= honor_form.label :get_date, "get_date" %>
				<%= text_field_tag 'profile_attribute[honor][][get_date]' %>
			</p>
			<p class="form-group">
				<%= honor_form.label :description, "description" %>
				<%= text_field_tag 'profile_attribute[honor][][description]' %>
			</p>
			<% end %>
		</div>
		<%=link_to '+','',{:id=>:honor_button}%>
	</div>
	<div class="research_experience">
		<div class="research_experience_item">
			<p>research_experience</p>
			<%= f.fields_for :experience do |experience_form| %>
			<p class="form-group">
				<%= experience_form.label :institude, "institude" %>
				<%= text_field_tag 'experiences[institude][1]' %>
			</p>
			<p class="form-group">
				<%= experience_form.label :title, "title" %>
				<%= text_field_tag 'experiences[title][1]' %>
			</p>
			<p class="form-group">
				<%= experience_form.label :date_from, "date_from" %>
				<%= text_field_tag 'experiences[date_from][1]' %>
			</p>
			<p class="form-group">
				<%= experience_form.label :date_to, "date_to" %>
				<%= text_field_tag 'experiences[date_to][1]' %>
				<%= hidden_field_tag 'experiences[type][1]', 'research' %>
			</p>
			<% end %>
		</div>
		<%=link_to '+','',{:id=>:research_experience}%>
	</div>
	<div class="work_experience">
		<div class="work_experience">
			<p>work_experience</p>
			<%= f.fields_for :experience do |experience_form| %>
			<p class="form-group">
				<%= experience_form.label :institude, "institude" %>
				<%= text_field_tag 'experiences[institude][1]' %>
			</p>
			<p class="form-group">
				<%= experience_form.label :title, "title" %>
				<%= text_field_tag 'experiences[title][1]' %>
			</p>
			<p class="form-group">
				<%= experience_form.label :date_from, "date_from" %>
				<%= text_field_tag 'experiences[date_from][1]' %>
			</p>
			<p class="form-group">
				<%= experience_form.label :date_to, "date_to" %>
				<%= text_field_tag 'experiences[date_to][1]' %>
				<%= hidden_field_tag 'experiences[type][1]', 'work' %>
			</p>
			<% end %>
		</div>
		<%=link_to '+','',{:id=>:work_experience}%>
	</div>
	<div class="button">
		<%= f.submit :submit %>
	</div>
<% end %>
<script>
$("input[type='text']").val('123');
$("#research_experience").on("click",function(e){
	$(".research_experience").append('132');
});

</script>