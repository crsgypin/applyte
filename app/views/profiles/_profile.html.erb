			<p class="form-group">
				title:<%= ef.title%>
			</p>
			<p class="form-group">
				date_from:<%= ef.date_from%>
			</p>
			<p class="form-group">
				date_to:<%= ef.date_to%>
			</p>
			<% end %>
		</div>
		</p>
